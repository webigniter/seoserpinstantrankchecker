{
    "manifest_version": 2,
    "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
    "background": {
        "page": "bg.htm",
        "persistent": true
    },
    "browser_action": {
        "default_icon": "gfx/icon19.png",
        "name": "SEO SERP",
        "default_popup": "popup.html",
        "permissions": ["background"]
    },
    "description": "",
    "icons": {
        "128": "gfx/icon128.png",
        "16": "gfx/icon16.png",
        "32": "gfx/icon32.png",
        "48": "gfx/icon48.png"
    },
    "name": "SEO SERP | INSTANT RANK CHECKER",
    "permissions": ["http://*/*", "https://*/*", "tabs", "chrome://favicon/", "unlimitedStorage", "idle", "activeTab", "background"],
    "version": "0.15.1"
}